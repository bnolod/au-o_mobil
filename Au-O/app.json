{
  "expo": {
    "name": "Au-O",
    "slug": "Au-O",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/auo-icon.png",
    "backgroundColor": "#1B1B1B",
    "scheme": "auo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/auo-icon.png",
        "backgroundColor": "#EF1A2D"
      },
      "package": "com.auo.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/auo-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/auo-logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#EF1A2D"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
